{
  "name": "the-resend-cli",
  "version": "0.1.2",
  "description": "CLI for the Resend email API",
  "type": "module",
  "bin": {
    "resend": "dist/main.js"
  },
  "files": ["dist", "CONTRIBUTING.md", "CODE_OF_CONDUCT.md", "SECURITY.md"],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "keywords": ["resend", "email", "cli"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shubham-Rasal/resend-cli.git"
  },
  "homepage": "https://github.com/Shubham-Rasal/resend-cli#readme",
  "bugs": {
    "url": "https://github.com/Shubham-Rasal/resend-cli/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "resend": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  }
}
